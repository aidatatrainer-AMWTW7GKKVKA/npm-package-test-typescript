{
    "name": "@smart-warehousing/tsx-sandbox-ui",
    "version": "0.2.17",
    "description": "A simple test package",
    "typings": "types/types.d.ts",
    "keywords": [
        "components"
    ],
    "main": "dist/index.js",
    "files": [
        "dist/",
        "README.md",
        "package.json"
    ],
    "scripts": {
        "preinstall": "npm run co:login",
        "co:login": "aws codeartifact login --tool npm --repository ui-collection --domain smart-warehousing-codeartifact-sandbox-domain  --region us-east-1 --profile smart-sandbox",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npm run build:babel && npm run build:types && npm run build:css",
        "build:babel": "rm -rf dist && ./node_modules/.bin/babel --extensions .tsx --extensions .ts src --out-dir ./dist --ignore '**/types'",
        "build:types": "python3 scripts/types_move.py",
        "build:css": "python3 scripts/css_move.py"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.10",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6"
    },
    "dependencies": {
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.5",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "antd": "^4.22.4",
        "create-react-app": "^5.0.1"
    }
}
